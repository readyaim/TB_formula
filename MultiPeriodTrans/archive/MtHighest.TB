//------------------------------------------------------------------------
// 简称: MtHigh
// 名称: 
// 类别: 用户函数
// 类型: 用户函数
// 输出: 数值型
// V1.1 
//------------------------------------------------------------------------
 
Params
	NumericSeries Price(1);
	NumericSeries BarCnt(0);
	Numeric Length(10);
Vars
	NumericSeries mtHigh(0);
	Numeric i;
	Numeric j(0);
Begin
	mtHigh = Price;
	For i = 1 to Length-1
	{
	        If (Price[j] <> InvalidNumeric)
	        {
	                if (mtHigh<Price[j]) 	mtHigh=Price[j];
	                j = j + BarCnt[j];
	        }
	        else Break;
	}
	Return mtHigh;
End

